{
  "extends": ["gitlab>dwp/engineering/renovate//shared-configs/renovate-auto-merge"],
  "baseBranches": [
    "main",
    "maintain/6.x",
    "maintain/7.x"
  ],
  "ignoreDeps": [
    "@dwp/govuk-casa"
  ],
  "packageRules": [{
    "description": "v6 supports up to node v12. These pacages require >=v14 so cannot be upgraded",
    "matchBaseBranches": ["maintain/6.x"],
    "matchPackageNames": ["fs-extra", "jsdom", "@stryker-mutator/core", "@stryker-mutator/mocha-runner"],
    "enabled": false
  }]
}
